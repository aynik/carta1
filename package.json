{
  "name": "carta1",
  "version": "1.1.0",
  "description": "An ATRAC1 audio codec implementation in JS",
  "type": "module",
  "main": "codec/index.js",
  "bin": {
    "carta1": "bin/cli.js"
  },
  "files": [
    "bin/",
    "codec/",
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "start": "node bin/cli.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepare": "husky",
    "lint": "eslint bin/ codec/ tests/",
    "format": "prettier --write .",
    "build": "webpack --config webpack.config.js",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "dependencies": {
    "cli-progress": "^3.12.0",
    "colors": "^1.4.0",
    "commander": "^11.0.0",
    "wav": "^1.0.2"
  },
  "engines": {
    "node": ">=20.16.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aynik/carta1.git"
  },
  "keywords": [
    "atrac",
    "atrac1",
    "audio",
    "codec",
    "minidisc",
    "compression",
    "encoder",
    "decoder"
  ],
  "author": "aynik",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aynik/carta1/issues"
  },
  "homepage": "https://github.com/aynik/carta1#readme",
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@babel/preset-env": "^7.27.2",
    "babel-loader": "^10.0.0",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "terser-webpack-plugin": "^5.3.14",
    "vitest": "^3.2.4",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  },
  "lint-staged": {
    "(bin|codec|tests)/**/*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
